  "main": "dist/server/index.js",
  "scripts": {
    "migrate": "knex migrate:latest",
    "build": "vite build --base ./",
    "docker": "bash ./scripts/start.bash",
    "preinstall": "ncu",
    "sort": "npx sort-package-json",
    "start": "concurrently -n client,server 'pnpm:start:client' 'pnpm:start:server'",
    "start:client": "ts-node-esm -P tsconfig.server.json vite.server.ts",
    "start:server": "ts-node -r tsconfig-paths/register dist/server/index.js",
    "dev": "nodemon --ext .ts,.html --watch src --watch vite.server.ts --exec 'pnpm start'"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
